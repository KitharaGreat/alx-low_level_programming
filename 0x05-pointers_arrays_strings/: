#include "main.h"
/**
 * rev_string - function to reverse a string
 * @s: parameter string
 * Return: 0
 */
void rev_string(char *s)
{
	char s[1000];;
	int i, j, count = 0;

	while (s[count] != '\0')
	{
		count++;
	}
	j = count - 1;
	char rev[1000];

	for (i = 0 ; i < count ; i++)
	{
		rev[i] = s[j];
		j--;
	}
}
